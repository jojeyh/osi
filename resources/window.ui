<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <template class="OSIWindow" parent="GtkApplicationWindow">
    <property name="width-request">360</property>
    <property name="title" translatable="no">OSI</property>
    <child>
      <object class="GtkBox">
        <property name="orientation">vertical</property>
        <property name="margin-top">12</property>
        <property name="margin-bottom">12</property>
        <property name="margin-start">12</property>
        <property name="margin-end">12</property>
        <property name="spacing">6</property>
        <child>
          <object class="GtkTextView" id="transcribed_text">
            <property name="visible">True</property>
            <property name="editable">False</property>
            <property name="wrap-mode">word</property>
          </object>
        </child>
        <child>
          <object class="GtkButton" id="transcribe_btn">
            <property name="label">Transcribe</property>
            <property name="visible">True</property>
          </object>
        </child>
      </object>
    </child>
  </template>
</interface>